{
  "name": "client-side",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run client-side:serve-ssr --host 0.0.0.0",
    "serve:ssr": "node dist/client-side/server/main.js",
    "build:ssr": "ng build --prod && ng run client-side:server:production",
    "build-and-serve": "npm run build:ssr && npm run start:prod",
    "prerender": "ng run client-side:prerender",
    "start:prod": "pm2 start dist/client-side/server/main.js --name \"official-web-client\"",
    "stop:prod": "pm2 stop official-web-client",
    "restart:prod": "pm2 restart official-web-client",
    "pm2:delete": "pm2 delete official-web-client"
  },
